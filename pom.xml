<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>me.eccentric_nz.dynmaptardis</groupId>
    <artifactId>Dynmap-TARDIS</artifactId>
    <version>1.3</version>
    <name>Dynmap-TARDIS</name>
    <repositories>
        <repository>
            <id>Spigot</id>
            <url>https://hub.spigotmc.org/nexus/content/groups/public/</url>
        </repository>
        <repository>
            <id>Bromley Massive</id>
            <url>http://tardisjenkins.duckdns.org:8080/plugin/repository/everything/</url>
        </repository>
    </repositories>
    <!-- Profiles are used to detect whether this is a local or Jenkins build
    and adjust the build number accordingly -->
    <profiles>
        <profile>
            <id>jenkins</id>
            <repositories>
                <repository>
                    <id>macgeek</id>
                    <url>http://localhost:8888</url>
                </repository>
            </repositories>
        </profile>
    </profiles>
    <build>
        <finalName>Dynmap-TARDIS</finalName>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.1</version>
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
    <dependencies>
        <dependency>
            <groupId>org.spigotmc</groupId>
            <artifactId>spigot-api</artifactId>
            <version>1.13.1-R0.1-SNAPSHOT</version>
            <type>jar</type>
            <scope>compile</scope>
            <exclusions>
                <exclusion>
                    <groupId>org.avaje</groupId>
                    <artifactId>ebean</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>org.hamcrest</groupId>
                    <artifactId>hamcrest-core</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>junit</groupId>
                    <artifactId>junit</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>com.google.code.gson</groupId>
                    <artifactId>gson</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>com.google.guava</groupId>
                    <artifactId>guava</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>com.googlecode.json-simple</groupId>
                    <artifactId>json-simple</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>org.yaml</groupId>
                    <artifactId>snakeyaml</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>commons-lang</groupId>
                    <artifactId>commons-lang</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>org.dynmap</groupId>
            <artifactId>dynmap-api</artifactId>
            <version>3.0-SNAPSHOT</version>
            <scope>system</scope>
            <systemPath>${basedir}/lib/dynmap-api-3.0-SNAPSHOT.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>me.eccentric_nz.TARDIS</groupId>
            <artifactId>TARDIS</artifactId>
            <version>4.0.3</version>
            <exclusions>
                <exclusion>
                    <artifactId>GriefPrevention</artifactId>
                    <groupId>me.ryanhamshire</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>LibsDisguises</artifactId>
                    <groupId>LibsDisguises</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>MultiWorld</artifactId>
                    <groupId>multiworld</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>Multiverse-Adventure</artifactId>
                    <groupId>com.onarandombox.multiverseadventure</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>Multiverse-Core</artifactId>
                    <groupId>com.onarandombox.multiversecore</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>Multiverse-Inventories</artifactId>
                    <groupId>com.onarandombox.multiverseinventories</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>ProtocolLib</artifactId>
                    <groupId>com.comphenix.protocol</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>TARDISChunkGenerator</artifactId>
                    <groupId>me.eccentric_nz.tardischunkgenerator</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>Towny</artifactId>
                    <groupId>com.palmergames.bukkit</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>WorldBorder</artifactId>
                    <groupId>com.wimbli.WorldBorder</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>citizensapi</artifactId>
                    <groupId>net.citizensnpcs</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>factions</artifactId>
                    <groupId>com.massivecraft</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>lockette</artifactId>
                    <groupId>org.yi.acru.bukkit</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>mcore</artifactId>
                    <groupId>com.massivecraft</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>worldedit</artifactId>
                    <groupId>com.sk89q</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>worldguard</artifactId>
                    <groupId>com.sk89q</groupId>
                </exclusion>
                <exclusion>
                    <groupId>com.massivecraft</groupId>
                    <artifactId>massivecore</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>org.swinglabs</groupId>
                    <artifactId>swing-layout</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
    </dependencies>
</project>
