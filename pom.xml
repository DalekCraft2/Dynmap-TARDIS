<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>me.eccentric_nz.dynmaptardis</groupId>
    <artifactId>Dynmap-TARDIS</artifactId>
    <version>1.1</version>
    <name>Dynmap-TARDIS</name>
    <repositories>
        <repository>
            <id>macgeek</id>
            <url>http://tardisjenkins.duckdns.org:8888</url>
        </repository>
        <repository>
            <id>Bromley Massive</id>
            <url>http://tardisjenkins.duckdns.org:8080/plugin/repository/everything/</url>
        </repository>
        <repository>
            <releases>
            </releases>
            <snapshots>
            </snapshots>
            <id>dynmap-repo</id>
            <url>http://repo.mikeprimm.com/</url>
        </repository>
    </repositories>
    <build>
        <finalName>DynMap-TARDIS</finalName>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.1</version>
                <configuration>
                    <source>1.6</source>
                    <target>1.6</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
    <dependencies>
        <dependency>
            <groupId>org.bukkit</groupId>
            <artifactId>bukkit</artifactId>
            <version>1.8-R0.1-SNAPSHOT</version>
            <type>jar</type>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>org.dynmap</groupId>
            <artifactId>dynmap-api</artifactId>
            <version>2.0</version>
        </dependency>
        <dependency>
            <groupId>me.eccentric_nz.TARDIS</groupId>
            <artifactId>TARDIS</artifactId>
            <version>3.2-beta-1</version>
            <exclusions>
                <exclusion>
                    <artifactId>GriefPrevention</artifactId>
                    <groupId>me.ryanhamshire</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>LibsDisguises</artifactId>
                    <groupId>LibsDisguises</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>MultiWorld</artifactId>
                    <groupId>multiworld</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>Multiverse-Adventure</artifactId>
                    <groupId>com.onarandombox.multiverseadventure</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>Multiverse-Core</artifactId>
                    <groupId>com.onarandombox.multiversecore</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>Multiverse-Inventories</artifactId>
                    <groupId>com.onarandombox.multiverseinventories</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>Project_Rassilon</artifactId>
                    <groupId>net.pekkit</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>ProtocolLib</artifactId>
                    <groupId>com.comphenix.protocol</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>TARDISChunkGenerator</artifactId>
                    <groupId>me.eccentric_nz.tardischunkgenerator</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>TARDISHorseSpeed</artifactId>
                    <groupId>me.eccentric_nz.tardishorsespeed</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>Towny</artifactId>
                    <groupId>com.palmergames.bukkit</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>WorldBorder</artifactId>
                    <groupId>com.wimbli.WorldBorder</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>citizensapi</artifactId>
                    <groupId>net.citizensnpcs</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>factions</artifactId>
                    <groupId>com.massivecraft</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>lockette</artifactId>
                    <groupId>org.yi.acru.bukkit</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>lwc</artifactId>
                    <groupId>com.griefcraft</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>mcore</artifactId>
                    <groupId>com.massivecraft</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>worldedit</artifactId>
                    <groupId>com.sk89q</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>worldguard</artifactId>
                    <groupId>com.sk89q</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>BarAPI</artifactId>
                    <groupId>me.confuser</groupId>
                </exclusion>
            </exclusions>
        </dependency>
    </dependencies>
</project>